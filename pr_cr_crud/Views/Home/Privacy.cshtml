@{
    ViewData["Title"] = "Privacy Policy";
}
<h1>@ViewData["Title"]</h1>

<p>Use this page to detail your site's privacy policy.</p>

@model List<pr_cr_crud.db_context.Owner>

<table>
    <tr>
        <th>Owner ID</th>
        <th> USer_Name</th>
        <th>Password</th>
        
    </tr>
    @foreach(var item in Model)
    {
        <tr>
        <td>@item.Id</td>
        <td>@item.UsrName</td>
        <td>@item.Password</td>
       
        </tr>
    }
    </table>